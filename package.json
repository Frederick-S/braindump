  "license": "MIT",
  "bugs": {
    "url": "https://github.com/levlaz/braindump/issues"
  },
  "homepage": "https://github.com/levlaz/braindump#readme",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "bootstrap-tagsinput": "^0.7.1",
    "jquery": "^2.2.3",
    "prosemirror": "^0.6.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "uglifyjs": "^2.4.10"
